  "description": "GPU Compression Side-Channel Research - Dependency Management",
  "scripts": {
    "setup": "bash install-dependencies.sh",
    "check": "bash check-system.sh",
    "build": "npm run build:poc && npm run build:llc",
    "build:poc": "cd poc/gpu-create && make",
    "build:llc": "cd 03-llc && make",
